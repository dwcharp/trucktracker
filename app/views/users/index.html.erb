<h1>Listing Users</h1>

<table>
  <tr>
    <th>User Name</th>
    <th>First Name</th>
    <th>Last Name</th>
    <th>Role</th>
  </tr>

  <% @users.each do |user| %>
      <tr>
        <td><%= user.user_name %></td>
        <td><%= user.first_name %></td>
        <td><%= user.last_name %></td>
        <td><%= user.get_role() %></td>
      </tr>
  <% end %>
</table>