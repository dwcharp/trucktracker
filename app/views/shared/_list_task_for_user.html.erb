<h1>Assigned Task</h1>

<table class="table" id="task-table">
  <tr>
    <th>Booking Number</th>
    <th>Loading Point</th>
    <th>Edit</th>
  </tr>

  <% tasks.each do |task| %>
      <tr>
        <td><%= task.booking_number %></td>
        <td><%= task.loading_point %></td>

      </tr>
  <% end %>
</table>
